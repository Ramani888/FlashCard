{
  "name": "FlashCard",
  "version": "1.1.6",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:release": "cd android && ./gradlew assembleRelease",
    "android:bundle": "cd android && ./gradlew bundleRelease",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "cd android && ./gradlew clean && cd .. && rm -rf node_modules/.cache",
    "clean:all": "rm -rf node_modules && rm -rf ios/Pods && npm install && cd ios && pod install",
    "pods": "cd ios && pod install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.0.0",
    "@react-native-clipboard/clipboard": "^1.15.0",
    "@react-native-community/blur": "^4.4.1",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-firebase/app": "^21.6.0",
    "@react-native-firebase/firestore": "^21.7.4",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "@reduxjs/toolkit": "^2.2.7",
    "@rneui/themed": "^4.0.0-rc.8",
    "formik": "^2.4.6",
    "lottie-react-native": "^7.2.2",
    "moment": "^2.30.1",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-native": "0.72.4",
    "react-native-actions-sheet": "^0.9.7",
    "react-native-blob-util": "^0.19.11",
    "react-native-device-info": "^14.0.4",
    "react-native-document-picker": "^9.3.1",
    "react-native-draggable-flatlist": "^4.0.1",
    "react-native-gesture-handler": "^2.21.2",
    "react-native-google-mobile-ads": "^14.7.1",
    "react-native-iap": "^12.16.0",
    "react-native-image-picker": "^7.1.2",
    "react-native-indicators": "^0.17.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-localization": "^2.3.2",
    "react-native-lottie-splash-screen": "^1.1.2",
    "react-native-masonry-grid": "^0.0.3",
    "react-native-otp-textinput": "^1.1.6",
    "react-native-pdf": "^6.7.5",
    "react-native-pinchable": "^0.2.1",
    "react-native-popup-menu": "^0.16.1",
    "react-native-progress": "^5.0.1",
    "react-native-raw-bottom-sheet": "^3.0.0",
    "react-native-reanimated": "^3.15.0",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^4.14.0",
    "react-native-screens": "^4.3.0",
    "react-native-select-dropdown": "^4.0.1",
    "react-native-size-matters": "^0.4.2",
    "react-native-vector-icons": "^10.1.0",
    "react-redux": "^9.1.2",
    "toggle-switch-react-native": "^3.3.0",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.72.2",
    "@react-native/metro-config": "^0.72.11",
    "@testing-library/react-native": "^12.4.0",
    "@tsconfig/react-native": "^3.0.0",
    "@types/jest": "^29.5.0",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^3.2.0",
    "react-native-dotenv": "^3.4.9",
    "react-test-renderer": "18.2.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18"
  }
}
